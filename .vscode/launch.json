{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wails: Debug",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/AdaruDbTool.exe",
      "preLaunchTask": "wails: build debug",
      "env": {
        "CGO_ENABLED": "1",
        "ADARU_ENV": "dev"
      }
    },
    {
      "name": "Wails: Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickProcess}"
    }
  ]
}
